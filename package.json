{
	"name": "rende",
	"version": "1.0.0",
	"description": "Events App",
	"main": "index.tsx",
	"scripts": {
		"dev": "npm-run-all --parallel dev:webpack dev:tailwind",
		"dev:webpack": "webpack serve --mode development",
		"dev:tailwind": "npx tailwindcss -i ./src/index.css -o ./public/index.css --watch",
		"build": "npm-run-all --parallel build:webpack build:tailwind",
		"build:webpack": "webpack --mode production",
		"build:tailwind": "npx tailwindcss -i ./src/index.css -o ./public/index.css --minify"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gosueep/rende.git"
	},
	"author": "gosuep",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/gosueep/rende/issues"
	},
	"homepage": "https://github.com/gosueep/rende#readme",
	"devDependencies": {
		"@solidjs/router": "^0.8.3",
		"@types/react": "^18.2.21",
		"@types/react-dom": "^18.2.7",
		"babel-loader": "^9.1.3",
		"babel-preset-solid": "^1.7.7",
		"npm-run-all": "^4.1.5",
		"solid-js": "^1.7.11",
		"solid-start": "^0.3.5",
		"solid-toast": "^0.5.0",
		"tailwindcss": "^3.3.3",
		"ts-loader": "^9.4.4",
		"typescript": "^5.2.2",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"dependencies": {
		"tailwind-scrollbar-hide": "^1.1.7"
	}
}
