{
	"name": "rende",
	"version": "1.0.0",
	"description": "Events App",
	"main": "index.tsx",
	"scripts": {
		"dev": "npm-run-all --parallel dev:webpack dev:tailwind",
		"dev:webpack": "webpack serve --mode development",
		"dev:tailwind": "npx tailwindcss -i ./src/index.css -o ./public/index.css --watch",
		"build": "npm-run-all --parallel build:webpack build:tailwind",
		"build:webpack": "npx webpack",
		"build:tailwind": "npx tailwindcss -i ./src/index.css -o ./public/index.css --minify"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheGreatRambler/rende.git"
	},
	"author": "Johnny Sins",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/TheGreatRambler/rende/issues"
	},
	"homepage": "https://github.com/TheGreatRambler/rende#readme",
	"devDependencies": {
		"@solidjs/router": "^0.7.0",
		"@types/bcrypt": "^5.0.0",
		"babel-loader": "^9.1.2",
		"babel-preset-solid": "^1.6.10",
		"npm-run-all": "^4.1.5",
		"solid-js": "^1.6.12",
		"tailwindcss": "^3.2.7",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.11.1"
	},
	"dependencies": {
		"bcrypt": "^5.1.0"
	}
}
